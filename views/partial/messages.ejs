<%if(locals.Error) { %>

    <% Error.forEach(v=>{ %>

        <div class="alert alert-warning  p-0 m-0" role="alert"> <%= v.msg%></div>
    <%})%>
    
  
  <%}%>

  <% if(messages.error){ %>
    <div class="alert alert-warning p-0 m-0" role="alert"><%= messages.error %></div>
<%}%>

<% if(messages.success){ %>
    <div class="alert alert-success  p-0 m-0" role="alert"> <%= messages.success %></div>
<%}%>